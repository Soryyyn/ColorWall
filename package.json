{
  "name": "hexwall",
  "version": "1.0.4",
  "description": "generates a wallpaper & sets it everyday",
  "main": "./dist/main/Main.js",
  "types": "./dist/main/Main.d.ts",
  "bin": {
    "hexwall": "./dist/main/Main.js"
  },
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w hexwall",
    "watch": "tsc --watch",
    "build-win": "electron-packager . HexWall --platform win32 --icon ./single_icon.ico --prune --out ./builds --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soryyyn/HexWall.git"
  },
